{
    "name": "vkolesnev/mindboxv3xml",
    "description": "v3 xml for mindbox sdk",
    "type": "library",
    "require": {
        "php": "^5.6|^7.0",
        "psr/log": "^1.1",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "mikey179/vfsstream": "*",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {"Mindbox\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Mindbox\\Tests\\": "tests/"}
    }    
}
